{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.5",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^7.2",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel-lang/common": "^6.7.1",
    "laravel-lang/native-locale-names": "^2.6.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^v5.21",
    "laravel/tinker": "^2.8",
    "socialiteproviders/crowdin": "^5.0",
    "socialiteproviders/discord": "^4.2",
    "spatie/laravel-google-fonts": "^1.4.4",
    "tightenco/ziggy": "^v2.6.0",
    "zonuexe/http-accept-language": "~0.6.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "dev-fix-alias-fake-error",
    "fakerphp/faker": "^1.9.1",
    "laravel-lang/lang": "^15.26.2",
    "laravel/breeze": "^2.0",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": "^12.4",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2.0"
  },
  "repositories": [
    {
      "url": "https://github.com/WentTheFox/laravel-ide-helper.git",
      "type": "vcs",
      "package": "barryvdh/laravel-ide-helper"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan lang:update",
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
